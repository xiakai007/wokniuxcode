<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.woniu.springboot.portals.mapper.UserMapper">
    <select id="selectUserByAccPwd" resultType="User">
        SELECT * FROM k15_user WHERE account=#{account} AND password=#{password}
    </select>
</mapper>