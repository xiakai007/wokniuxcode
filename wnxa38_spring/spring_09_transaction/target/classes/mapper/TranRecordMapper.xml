<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.woniu.mapper.TranRecordMapper">
    <insert id="insertTranRecord" parameterType="TranRecord">
        INSERT INTO tran_record (type,money,message) VALUE (#{type},#{money},#{message})
    </insert>
</mapper>